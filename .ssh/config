Host github.com
    PreferredAuthentications publickey
    IdentityFile ~/.ssh/id_github
    IdentitiesOnly yes

Host gitlab.com
    PreferredAuthentications publickey
    IdentityFile ~/.ssh/id_gitlab
    IdentitiesOnly yes

Host home
    HostName home.ap.yuji.ne.jp
    User yuji
    ProxyCommand cloudflared access ssh --hostname %h
    PreferredAuthentications publickey
    IdentityFile ~/.ssh/id_home
    IdentitiesOnly yes

