{
    "update.showReleaseNotes": false,
    "telemetry.enableCrashReporter": false,
    "telemetry.enableTelemetry": false,
    "git.autofetch": true,
    "git.fetchOnPull": true,
    "git.suggestSmartCommit": false,
    "search.useGlobalIgnoreFiles": true,
    "github.gitAuthentication": false,
    "editor.minimap.enabled": false,
    "remote.restoreForwardedPorts": true,
    "git.confirmSync": false,
    "editor.renderControlCharacters": true,
    "C_Cpp.updateChannel": "Insiders",
    "editor.fontFamily": "'JetBrainsMono Nerd Font', 'FiraCode Nerd Font', 'Droid Sans Mono', 'monospace', monospace, 'Droid Sans Fallback'",
    "editor.fontLigatures": "'ss01', 'ss02', 'ss03', 'ss04', 'ss05', 'ss06', 'zero', 'onum'",
    "[rust]": {
        "editor.defaultFormatter": "matklad.rust-analyzer"
    },
    "[html]": {
        "editor.defaultFormatter": "vscode.html-language-features"
    },
    "remote.extensionKind": {
        "ms-azuretools.vscode-docker": "workspace"
    },
    "explorer.confirmDragAndDrop": false,
    "diffEditor.ignoreTrimWhitespace": false,
    "editor.largeFileOptimizations": false,
    "update.mode": "manual",
    "workbench.editorAssociations": {
        "*.ipynb": "jupyter-notebook"
    },
    "latex-workshop.view.pdf.viewer": "tab",
    "python.defaultInterpreterPath": "/opt/homebrew/opt/python@3.9/bin/python3.9",
    "notebook.cellToolbarLocation": {
        "default": "right",
        "jupyter-notebook": "left"
    },
    "python.pythonPath": "/opt/homebrew/bin/python3",
    "markdown.marp.themes": [
        "~/dotfiles/marp.css",
        "./marp.css"
    ],
    "workbench.colorTheme": "Quiet Light",
    "editor.foldingMaximumRegions": 65000
}